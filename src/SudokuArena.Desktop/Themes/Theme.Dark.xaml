<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="CanvasBackgroundColor">#1B1E24</Color>
    <Color x:Key="SurfaceColor">#252A33</Color>
    <Color x:Key="PrimaryColor">#70B7FF</Color>
    <Color x:Key="AccentColor">#FF9D7A</Color>
    <Color x:Key="TextColor">#E7EAF0</Color>

    <SolidColorBrush x:Key="CanvasBackgroundBrush" Color="{StaticResource CanvasBackgroundColor}" />
    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource SurfaceColor}" />
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource TextColor}" />

    <SolidColorBrush x:Key="BoardBackgroundBrush" Color="#20252E" />
    <SolidColorBrush x:Key="BoardRelatedGroupBrush" Color="#313843" />
    <SolidColorBrush x:Key="BoardActiveCellBrush" Color="#4A78C2" />
    <SolidColorBrush x:Key="BoardMatchingDigitBrush" Color="#4A78C2" />
    <SolidColorBrush x:Key="BoardCompletionPulseBrush" Color="#73A6FF" />
    <SolidColorBrush x:Key="BoardGridMajorBrush" Color="#93A0B5" />
    <SolidColorBrush x:Key="BoardGridMinorBrush" Color="#5B6677" />
    <SolidColorBrush x:Key="BoardConflictTextBrush" Color="#FF7A97" />
    <SolidColorBrush x:Key="BoardGivenTextBrush" Color="#F2F5FA" />
    <SolidColorBrush x:Key="BoardUserTextBrush" Color="#DDE3EC" />
</ResourceDictionary>
