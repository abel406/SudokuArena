<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="CanvasBackgroundColor">#F5F3EC</Color>
    <Color x:Key="SurfaceColor">#FFFDF8</Color>
    <Color x:Key="PrimaryColor">#1C6E8C</Color>
    <Color x:Key="AccentColor">#E07A5F</Color>
    <Color x:Key="TextColor">#2D3142</Color>

    <SolidColorBrush x:Key="CanvasBackgroundBrush" Color="{StaticResource CanvasBackgroundColor}" />
    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource SurfaceColor}" />
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="TextBrush" Color="{StaticResource TextColor}" />

    <SolidColorBrush x:Key="BoardBackgroundBrush" Color="#FFFFFF" />
    <SolidColorBrush x:Key="BoardRelatedGroupBrush" Color="#ECEFF4" />
    <SolidColorBrush x:Key="BoardActiveCellBrush" Color="#A9CBFF" />
    <SolidColorBrush x:Key="BoardMatchingDigitBrush" Color="#A9CBFF" />
    <SolidColorBrush x:Key="BoardCompletionPulseBrush" Color="#4A78C2" />
    <SolidColorBrush x:Key="BoardGridMajorBrush" Color="#768090" />
    <SolidColorBrush x:Key="BoardGridMinorBrush" Color="#C0C7D2" />
    <SolidColorBrush x:Key="BoardConflictTextBrush" Color="#D73359" />
    <SolidColorBrush x:Key="BoardGivenTextBrush" Color="#111827" />
    <SolidColorBrush x:Key="BoardUserTextBrush" Color="#1F2937" />
</ResourceDictionary>
